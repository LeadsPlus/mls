!!!
-# { :"xmlns:fb" => "http://www.facebook.com/2008/fbml" }
%html
  %head
    %title How Much House
    = render "layouts/fb_meta_tags"
    = javascript_include_tag :defaults, 'fancybox.pack'
    = stylesheet_link_tag 'styles', 'fancybox'
    = stylesheet_link_tag 'smoothness/jquery-ui'
    -#%script{ :src => "http://platform.twitter.com/widgets.js" }
    = csrf_meta_tag
  %body
    #wrapper
      #header
        = render 'layouts/header'
      #notice= notice
      #alert= alert
      = render 'shared/search_bar' if @search_form
      #content
        = yield
      #footer
        = render 'layouts/footer'
      = debug params if Rails.env.development?