!!!
%html{ :"xmlns:fb" => "http://www.facebook.com/2008/fbml" }
  %head
    %title Mls
    %meta{ :property => "og:title", :content => "How Much House" }
    %meta{ :property => "og:type", :content => "website" }
    %meta{ :property => "og:url", :content => "http://www.howmuchhouse.net/" }
    %meta{ :property => "og:image", :content => "" }
    %meta{ :property => "og:site_name", :content => "http://www.howmuchhouse.net/" }
    %meta{ :property => "og:app_id", :content => "211207382236306" }
    = stylesheet_link_tag :all
    = stylesheet_link_tag 'smoothness/jquery-ui'
    = javascript_include_tag :defaults, 'analytics'
    %script{ :src => "http://platform.twitter.com/widgets.js" }
    = csrf_meta_tag
  %body
    #wrapper
      #header
        = render 'layouts/header'
      = render 'shared/search_bar' if @search_form
      #content
        = yield
      #footer
      = debug params if Rails.env.development?